<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Controller">
   <children>
      <TableView fx:id="tableView" prefHeight="349.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="36.0" text="id" />
          <TableColumn fx:id="nameColumn" prefWidth="394.0" text="name" />
            <TableColumn fx:id="ageColumn" prefWidth="169.0" text="age" />
        </columns>
      </TableView>
      <Button fx:id="updateButton" layoutX="80.0" layoutY="361.0" mnemonicParsing="false" onAction="#update" text="Update" />
      <Button fx:id="createButton" layoutX="281.0" layoutY="361.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button fx:id="deleteButton" layoutX="462.0" layoutY="361.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
   </children>
</AnchorPane>
